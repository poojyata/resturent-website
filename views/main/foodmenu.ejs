<style>
    @import url(https://fonts.googleapis.com/css?family=Cinzel+Decorative);
    @font-face {
        font-family: 'Calluna-Regular';
        src: url('https://dl.dropboxusercontent.com/u/166051553/Calluna-Regular/2A2DE3_0_0.eot');
        src: url('https://dl.dropboxusercontent.com/u/166051553/Calluna-Regular/2A2DE3_0_0.eot?#iefix') format('embedded-opentype'), url('https://dl.dropboxusercontent.com/u/166051553/Calluna-Regular/2A2DE3_0_0.woff') format('woff'), url('https://dl.dropboxusercontent.com/u/166051553/Calluna-Regular/2A2DE3_0_0.ttf') format('truetype');
    }
    
    .chtml {
        /* margin-top: -2em; */
        padding: 0;
        border: 0 none;
        position: relative;
    }
    
    .chtml {
        color: black;
        margin: 2rem;
        border: 4px solid;
        /* margin-top: -10em; */
        width: calc(100% - 36px);
        max-width: 800px;
        /* padding: 16px; */
        font-family: calluna-regular, 'Cinzel Decorative', serif;
        font-size: 1rem;
    }
    
    .bbody {
        width: 80%;
        margin-top: 2em;
        margin-left: 8em;
        border: 4px solid;
        border-radius: 10px;
        border-color: black;
    }
    
    .chtml:before,
    .chtml:after,
    .bbody:before,
    .bbody:after {
        content: '';
        position: absolute;
        width: 42px;
        height: 42px;
        border: 4px solid;
    }
    
    .chtml:before {
        top: -4px;
        left: -4px;
        border-color: red whitesmoke red whitesmoke;
        border-radius: 0 0 5px 0;
    }
    
    .chtml:after {
        top: -4px;
        right: -4px;
        border-color: red whitesmoke red whitesmoke;
        border-radius: 0 0 0 5px;
    }
    
    .bbody:before {
        bottom: -8em;
        left: 23em;
        border-color: red whitesmoke red whitesmoke;
        border-radius: 0 5px 0 0;
    }
    
    .bbody:after {
        bottom: -8em;
        right: 22em;
        border-color: red whitesmoke red whitesmoke;
        border-radius: 5px 0 0 0;
    }
    
    .Ah1 {
        text-align: center;
        font-size: 3rem;
        color: red;
        font-family: 'Cinzel Decorative', serif;
        -webkit-font-feature-settings: "clig", "dlig", "hlig", "liga", "swsh", "onum";
        -moz-font-feature-settings: "clig", "dlig", "hlig", "liga", "swsh", "onum";
        font-feature-settings: "clig", "dlig", "hlig", "liga", "swsh";
        padding-top: 1rem;
    }
    
    article p:nth-of-type(1) {
        padding-left: 1rem;
        color: black;
        margin-bottom: 1rem;
        line-height: 1.3;
        font-size: 1.9rem;
    }
    
    article h2+p:first-letter {
        background: black;
        font-family: 'Cinzel Decorative', serif;
        float: left;
        font-size: 7rem;
        color: seagreen;
        line-height: 100%;
        margin-top: -7rem;
        margin-left: -7rem;
        border-radius: 50%;
        text-shadow: 1px 1px 0 whitesmoke;
        box-shadow: 0 0 0 5px #e5e5e5;
    }
    
    .p1:before {
        color: seagreen;
        font-size: 5rem;
        position: absolute;
        top: -2rem;
        left: 50%;
        opacity: .6;
    }
    
    .ulli {
        color: seagreen;
        font-family: cursive;
        font-size: 15px;
    }
    
    .allboxsize {
        margin-top: -5em;
        margin-left: 50em;
        margin-bottom: 5em;
        height: 4em;
    }
    
    .imgleftside {
        margin-top: 3em;
        margin-left: 7em;
        border-radius: 10px;
        border-style: solid;
    }
    
    .imgrightside {
        margin-top: -5em;
        margin-left: -70em;
        border-radius: 10px;
        border-style: solid;
    }
</style>

